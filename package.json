{
  "name": "main-api",
  "version": "0.0.0",
  "description": "main api mylight-systems",
  "author": "",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon | bunyan --color",
    "prestart:ci": "tsc && copyfiles --flat src/**/*.graphql dist/graphql/",
    "prestart:copyfiles": "copyfiles --flat src/**/*.graphql dist/graphql/",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "start:hmr": "node dist/server",
    "start:localprod": "cross-env NODE_ENV=production node dist/main.js",
    "test": "cross-env NODE_ENV=test LOG_LEVEL=quiet jest",
    "test:debug": "cross-env NODE_ENV=test DEBUG=nock.* jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "migration:create": "node create_migration",
    "migration:run": "ts-node node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert",
    "migration:seed": "node_modules/ts-node/dist/bin.js seed/seed_runner.ts",
    "migration:prodseed": "node dits/seed/seed_runner.js"
  },
  "dependencies": {
    "@nestjs/common": "^5.3.6",
    "@nestjs/core": "^5.3.6",
    "@nestjs/jwt": "^0.2.0",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/typeorm": "^5.0.2",
    "@types/moment": "^2.13.0",
    "applicationinsights": "^1.0.3",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "class-validator": "^0.9.1",
    "copyfiles": "^2.0.0",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "fastify-formbody": "^2.0.0",
    "joi": "^13.4.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "lodash.get": "^4.4.2",
    "moment": "^2.22.2",
    "mssql": "^4.1.0",
    "mysql": "^2.16.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.6.1",
    "reflect-metadata": "^0.1.12",
    "regenerator-runtime": "^0.12.1",
    "rxjs": "^6.3.3",
    "ts-node": "^4.1.0",
    "typeorm": "^0.2.7",
    "typescript": "^2.9.2",
    "urijs": "^1.19.1",
    "zen-observable": "^0.8.9"
  },
  "devDependencies": {
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/testing": "^5.1.0",
    "@types/bunyan": "^1.8.4",
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/nock": "^9.3.0",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "@types/urijs": "^1.15.38",
    "cross-env": "^5.2.0",
    "jest": "^21.2.1",
    "nock": "^9.4.4",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "sqlite3": "^4.0.2",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.3.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "node"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
